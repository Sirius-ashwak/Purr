{
  "name": "purr-app",
  "version": "1.0.0",
  "description": "Purr! - A modern, playful mobile app with a cute cat mascot that makes you purr with productivity",
  "main": "index.js",
  "scripts": {
    "start": "npx serve dist -s -l 3000",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform web --output-dir dist --clear",
    "build:web": "expo export --platform web --output-dir dist"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@expo/metro-runtime": "^5.0.4",
    "@lottiefiles/dotlottie-react": "^0.15.1",
