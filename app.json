{
  "expo": {
  "platforms": ["ios", "android"],
    "name": "PurrPlan World",
    "slug": "purrplan-world",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4a148c"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.purrplan.world"
    },
    "android": {
      "adaptiveIcon": {
  "foregroundImage": "./assets/adaptive-icon.png",
  "backgroundColor": "#4a148c"
      },
      "package": "com.purrplan.world"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [],
    "experiments": {
      "typedRoutes": false
    }
  }
}
