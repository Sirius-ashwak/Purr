{
  "expo": {
  "platforms": ["ios", "android", "web"],
    "name": "Purr!",
    "slug": "purr-app",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "light",
    
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.purr.app",
      "newArchEnabled": false
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#4a148c"
      },
      "package": "com.purr.app",
      "newArchEnabled": false
    },
    "web": {
      "bundler": "metro",
      "newArchEnabled": false
    },
    
    "plugins": [],
    "experiments": {
      "typedRoutes": false,
      "turboModules": false
    },
    "jsEngine": "hermes",
    "newArchEnabled": false
  }
}
